{
  "name": "@silencelaboratories/silent-shard-snap-staging",
  "version": "1.2.9-alpha.2",
  "author": "Silence Laboratories",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "Distributed wallet between Metamask plugin and SilentShard mobile app",
  "keywords": [
    "metamask",
    "snap",
    "keyring",
    "silent shard"
  ],
  "repository": {
    "type": "git",
    "url": "https://www.npmjs.com/package/@silencelaboratories/silent-shard-snap-staging"
  },
  "homepage": "https://www.npmjs.com/package/@silencelaboratories/silent-shard-snap-staging",
  "main": "dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:prod": "mm-snap build -c ./snap.config.ts",
    "build:stg": "mm-snap build -c ./snap.staging.config.ts",
    "build:clean:prod": "npm run clean && npm run build:prod",
    "build:clean:stg": "npm run clean && npm run build:stg",
    "prepublish:prod": "NODE_ENV='production' node ./scripts/pre-publish.js && npm i && npm run build:clean:prod",
    "prepublish:stg": "NODE_ENV='staging' node ./scripts/pre-publish.js && npm i && npm run build:clean:stg",
    "clean": "rimraf dist",
    "lint": "npm run lint:eslint && npm run lint:misc && npm run lint:deps && npm run lint:types",
    "lint:deps": "depcheck",
    "lint:eslint": "eslint . --cache --ext js,jsx,ts,tsx",
    "lint:fix": "npm run lint:eslint --fix && npm run lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '!snap.manifest.json' --check",
    "lint:types": "tsc --noEmit",
    "serve": "mm-snap serve",
    "start": "NODE_ENV='staging' node ./scripts/pre-publish.js && mm-snap watch -c ./snap.staging.config.ts",
    "test": "npm run prepublish:stg && jest -i"
  },
  "dependencies": {
    "@ethereumjs/common": "^3.1.2",
    "@ethereumjs/tx": "^4.1.2",
    "@ethereumjs/util": "^8.0.5",
    "@metamask/eth-sig-util": "^7.0.1",
    "@metamask/keyring-api": "^1.1.0",
    "@metamask/snaps-sdk": "^5.0.0",
    "@metamask/snaps-types": "^3.1.0",
    "@metamask/snaps-ui": "^3.1.0",
    "@metamask/utils": "^8.5.0",
    "@silencelaboratories/ecdsa-tss": "^1.1.3",
    "buffer": "^6.0.3",
    "keccak256": "^1.0.6",
    "libsodium-wrappers": "^0.7.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^3.3.0",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "^6.2.0",
    "@metamask/snaps-jest": "^8.1.2",
    "@types/jest": "^29.5.12",
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chalk": "^4.1.2",
    "depcheck": "^1.4.6",
    "dotenv": "^16.4.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "firebase": "^9.21.0",
    "jest": "^29.7.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "typescript": "~5.1"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}